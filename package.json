    "encoding": "^0.1.13",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.9",
    "netlify-lambda": "^1.6.3",
    "serverless-http": "^2.6.0",
    "terser-webpack-plugin": "^3.1.0",
